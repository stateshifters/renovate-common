{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "stylelint"
      ],
      "matchPackagePatterns": [
        ".*stylelint.*"
      ],
      "automerge": true,
      "groupName": "stylelint dependencies"
    }
  ]
}
