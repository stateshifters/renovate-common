{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "esbuild"
      ],
      "matchPackagePatterns": [
        ".*esbuild.*"
      ],
      "matchPackageNames": [
        "esbuild"
      ],
      "automerge": true,
      "groupName": "esbuild dependencies"
    }
  ]
}
