{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackagePrefixes": [
        "@vitejs/",
        "vite-"
      ],
      "matchPackageNames": [
        "vite"
      ],
      "automerge": true,
      "groupName": "vite dependencies"
    }
  ]
}
